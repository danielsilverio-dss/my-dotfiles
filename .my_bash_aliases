# My Aliases
#####

alias mvn-cpi="mvn clean package install"
alias doc="cd ~/Documentos"
alias dl="cd ~/Downloads"
alias l="ls -la"
#alias l="telnet towel.blinkenlights.nl"
alias lst="ls -lat"
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias logs="cd ~/scripts/logs"
alias logs-wildfly="cd /home/daniel/Dev/lib/wildfly-10.1.0.Final/standalone/log && tail -f -n 500 server.log"

alias gr="cd ~/git/resolucao-ws"
